    "import sqlite3\n",
    "\n",
    "conn = sqlite3.connect('spider-assignment-myurl.sqlite')\n",
    "cur = conn.cursor()\n",
    "\n",
    "cur.execute('''UPDATE Pages SET new_rank=1.0, old_rank=0.0''')\n",
    "conn.commit()\n",
    "\n",
    "cur.close()\n",
    "\n",
    "print(\"All pages set to a rank of 1.0\")"
   ]
